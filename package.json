{
    "name": "birthday-bot",
    "version": "1.0.0",
    "author": "Scott Bucher",
    "description": "Celebrate birthdays with automatic birthday roles and announcements! Highly customizable and 100% free!",
    "private": true,
    "engines": {
        "node": ">=12.14.0"
    },
    "main": "dist/start.js",
    "scripts": {
        "lint": "tslint --project .",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "build": "tsc",
        "start": "npm run build && node dist/start.js",
        "start:shard": "npm run build && node dist/app.js",
        "start:pm2": "npm run build && npm run pm2:start",
        "pm2:start": "pm2 start process.json",
        "pm2:stop": "pm2 stop process.json",
        "pm2:delete": "pm2 delete process.json"
    },
    "dependencies": {
        "chrono-node": "2.1.9",
        "color-name-list": "7.13.0",
        "discord.js": "12.4.1",
        "discord.js-collector-utils": "1.0.17",
        "express": "4.17.1",
        "express-promise-router": "^4.0.1",
        "limiter": "1.1.5",
        "moment-timezone": "0.5.31",
        "mysql": "2.18.1",
        "node-fetch": "2.6.1",
        "node-schedule": "1.3.2",
        "pm2": "^4.5.0"
    },
    "devDependencies": {
        "@types/express": "4.17.8",
        "@types/mysql": "2.15.15",
        "@types/node": "^14.14.3",
        "@types/node-fetch": "2.5.7",
        "@types/node-schedule": "1.3.1",
        "prettier": "^2.1.2",
        "tslint": "^6.1.3",
        "typescript": "^4.0.3"
    }
}
