{
    "name": "birthday-bot",
    "version": "1.0.0",
    "author": "Scott Bucher",
    "description": "A 100% free & highly customizable birthday Discord Bot. Use bday help to start tracking birthdays!",
    "engines": {
        "node": ">=12.14.0"
    },
    "main": "dist/start.js",
    "scripts": {
        "start": "tsc && node dist/start.js",
        "start-shard": "tsc && node dist/app.js",
        "start-pm2": "tsc && pm2 start process.json",
        "stop-pm2": "pm2 stop process.json",
        "delete-pm2": "pm2 delete process.json",
        "build": "tsc"
    },
    "dependencies": {
        "chrono-node": "1.4.6",
        "discord.js": "12.2.0",
        "discord.js-collector-utils": "1.0.9",
        "moment-timezone": "0.5.31",
        "mysql": "2.18.1",
        "node-schedule": "1.3.2",
        "pm2": "^4.4.0",
        "request": "2.88.2",
        "request-promise": "4.2.5"
    },
    "devDependencies": {
        "@types/mysql": "^2.15.14",
        "@types/node": "^14.0.18",
        "@types/node-schedule": "^1.3.0",
        "@types/request": "^2.48.5",
        "@types/request-promise": "^4.1.46",
        "tslint": "^6.1.2",
        "typescript": "^3.9.6"
    }
}
