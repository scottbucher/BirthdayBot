{
    "name": "birthday-bot",
    "version": "1.0.0",
    "author": "Scott Bucher",
    "description": "Celebrate birthdays with automatic birthday roles and announcements! Highly customizable and 100% free!",
    "engines": {
        "node": ">=12.14.0"
    },
    "main": "dist/start.js",
    "scripts": {
        "start": "tsc && node dist/start.js",
        "start-shard": "tsc && node dist/app.js",
        "start-pm2": "tsc && pm2 start process.json",
        "stop-pm2": "pm2 stop process.json",
        "delete-pm2": "pm2 delete process.json",
        "build": "tsc"
    },
    "dependencies": {
        "chrono-node": "2.1.8",
        "discord.js": "12.3.1",
        "discord.js-collector-utils": "1.0.11",
        "express": "4.17.1",
        "moment-timezone": "0.5.31",
        "mysql": "2.18.1",
        "node-fetch": "2.6.0",
        "node-schedule": "1.3.2",
        "pm2": "^4.4.1"
    },
    "devDependencies": {
        "@types/express": "4.17.7",
        "@types/mysql": "2.15.15",
        "@types/node": "^14.6.1",
        "@types/node-fetch": "2.5.7",
        "@types/node-schedule": "1.3.0",
        "nodemon": "^2.0.4",
        "ts-node": "^9.0.0",
        "tslint": "^6.1.3",
        "typescript": "^4.0.2"
    }
}
